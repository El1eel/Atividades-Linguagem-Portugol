programa {
  funcao inicio() {
    //dados
    cadeia nome = "El"
    inteiro idade = 18
    caracter sexo = 'M'
    real peso = 58.7
    real altura = 1.76
    cadeia cor = "Pardo"



    //exibir
    escreva("Nome: ", nome)
    escreva("\nIdade:" , idade)
    escreva("\nSexo:" , sexo)
    escreva("\nPeso:" , peso)
    escreva("\nAltura:" , altura)
    escreva("\nCor:" , cor)
  }
}
